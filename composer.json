{
    "name": "sure-lv/emails",
    "description": "Private shared email domain bundle",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.2",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/mailer": "^6.4|^7.0",
        "symfony/messenger": "^6.4|^7.0",
        "symfony/routing": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "ramsey/uuid": "^4.9",
        "doctrine/dbal": "^4.4",
        "symfony/twig-bundle": "^6.4|^7.0"
    },
    "suggest": {
        "symfony/rate-limiter": "Required if you want rate limiting on email sending"
    },
    "autoload": {
        "psr-4": {
            "SureLv\\Emails\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SureLv\\Emails\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "SureLv\\Emails\\Installer\\PackageInstaller::install1"
        ],
        "post-update-cmd": [
            "SureLv\\Emails\\Installer\\PackageInstaller::install1"
        ]
    }
}
